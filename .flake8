[flake8]
max-line-length = 127
max-complexity = 10
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    build,
    dist,
    *.egg-info,
    .pytest_cache,
    .coverage,
    gen/
ignore = 
    E203,
    E501,
    W503,
    F401,
    F403,
    F405,
    W293,
    E302,
    F811,
    F841,
    W191,
    E101
per-file-ignores =
    __init__.py:F401
    tests/__init__.py:F401
